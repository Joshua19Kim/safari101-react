{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build",
        "generateSourceMaps": true
      }
    },
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/static/$1"
    },
    {
      "src": "/.*map$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}